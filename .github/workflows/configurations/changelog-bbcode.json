{
  "categories": [
    {
      "title": "[h1]New Features[/h1]",
      "labels": [ "feature" ]
    },
    {
      "title": "[h1]Enhancements[/h1]",
      "labels": [ "enhancement" ]
    },
    {
      "title": "[h1]Bug Fixes[/h1]",
      "labels": [ "bug" ]
    }
  ],
  "sort": {
    "order": "ASC",
    "on_property": "mergedAt"
  },
  "template": "${{CHANGELOG}}",
  "pr_template": "[b]${{TITLE}}[/b] ([url=${{URL}}]PR #${{NUMBER}}[/url])\n${{BODY}}\n\n",
  "empty_template": "[i]No changes.[/i]"
}
